--// Rael Hub - Script único
--// Feito para Delta Executor
--// by ooncity_kkj

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Lighting = game:GetService("Lighting")
local Workspace = game:GetService("Workspace")
local StarterGui = game:GetService("StarterGui")

local state = {
    infJump = false,
    fly = false,
    flySpeed = 50,
    esp = false,
    noclip = false,
    wsBoost = 16,
    fullbright = false,
    antisit1 = false,
    antisit2 = false,
    anticollision = false,
    antivoid = false,
    antiaudio = false,
    antilag = false
}

-- Infinite Jump
UserInputService.JumpRequest:Connect(function()
    if state.infJump and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character:FindFirstChild("Humanoid"):ChangeState("Jumping")
    end
end)

-- Fly
local bodyVel, bodyGyro
local function flyToggle(on)
    local char = LocalPlayer.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end
    if on then
        local hrp = char.HumanoidRootPart
        bodyVel = Instance.new("BodyVelocity", hrp)
        bodyVel.MaxForce = Vector3.new(9e9,9e9,9e9)
        bodyVel.Velocity = Vector3.new(0,0,0)

        bodyGyro = Instance.new("BodyGyro", hrp)
        bodyGyro.MaxTorque = Vector3.new(9e9,9e9,9e9)

        RunService.Heartbeat:Connect(function()
            if state.fly and bodyVel and bodyGyro then
                local cam = workspace.CurrentCamera
                bodyGyro.CFrame = cam.CFrame
                bodyVel.Velocity = cam.CFrame.LookVector * state.flySpeed
            end
        end)
    else
        if bodyVel then bodyVel:Destroy() bodyVel=nil end
        if bodyGyro then bodyGyro:Destroy() bodyGyro=nil end
    end
end

-- ESP
local function toggleESP(on)
    for _,plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            local char = plr.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                if on then
                    if not char:FindFirstChild("RaelESP") then
                        local h = Instance.new("Highlight")
                        h.Name = "RaelESP"
                        h.FillTransparency = 0.6
                        h.OutlineColor = Color3.fromRGB(0,255,0)
                        h.Parent = char
                    end
                else
                    if char:FindFirstChild("RaelESP") then
                        char:FindFirstChild("RaelESP"):Destroy()
                    end
                end
            end
        end
    end
end

-- FullBright
local function toggleFullBright(on)
    if on then
        Lighting.Brightness = 2
        Lighting.ClockTime = 14
        Lighting.FogEnd = 1e6
        Lighting.GlobalShadows = false
        Lighting.OutdoorAmbient = Color3.new(1,1,1)
    else
        Lighting.GlobalShadows = true
        Lighting.ClockTime = 0
    end
end

-- Anti Sit v1
local function antiSit1()
    if state.antisit1 and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.Sit = false
    end
end
RunService.Heartbeat:Connect(antiSit1)

-- Anti Sit v2
local function antiSit2()
    if state.antisit2 and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.SeatPart = nil
        LocalPlayer.Character.Humanoid.Sit = false
    end
end
RunService.Heartbeat:Connect(antiSit2)

-- Anti Hack (simples, remove localscripts estranhos no player)
local function antiHack()
    for _,obj in pairs(LocalPlayer.PlayerScripts:GetChildren()) do
        if obj:IsA("LocalScript") and not obj.Name:match("Rael") then
            obj:Destroy()
        end
    end
end

-- Anti Collision
RunService.Stepped:Connect(function()
    if state.anticollision and LocalPlayer.Character then
        for _,part in pairs(LocalPlayer.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
end)

-- Anti Void
local voidPart
local function toggleAntiVoid(on)
    if on then
        if not voidPart then
            voidPart = Instance.new("Part", Workspace)
            voidPart.Size = Vector3.new(5000,1,5000)
            voidPart.Position = Vector3.new(0,-20,0)
            voidPart.Anchored = true
            voidPart.Transparency = 1
        end
    else
        if voidPart then voidPart:Destroy() voidPart=nil end
    end
end

-- Anti Audio All
local function toggleAntiAudio(on)
    for _,s in pairs(Workspace:GetDescendants()) do
        if s:IsA("Sound") and s.Playing then
            if on then
                if s.Parent ~= LocalPlayer.Character then
                    s.Volume = 0
                end
            else
                s.Volume = 1
            end
        end
    end
end

-- Anti Lag
local function antiLagLoop()
    if state.antilag then
        for _,v in pairs(Workspace:GetDescendants()) do
            if v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Explosion") then
                v:Destroy()
            end
        end
    end
end
RunService.Heartbeat:Connect(antiLagLoop)

-- Anti AFK
for _,v in pairs(getconnections(LocalPlayer.Idled)) do
    v:Disable()
end

--// Interface RayField
local Window = Rayfield:CreateWindow({
    Name = "Rael Hub",
    LoadingTitle = "Rael Hub",
    LoadingSubtitle = "by ooncity_kkj",
    ConfigurationSaving = {
        Enabled = true,
        FileName = "RaelHub"
    }
})

-- Aba Movement
local TabMove = Window:CreateTab("Movement")
TabMove:CreateToggle({
    Name = "Infinite Jump",
    CurrentValue = false,
    Callback = function(v) state.infJump = v end
})
TabMove:CreateToggle({
    Name = "Fly",
    CurrentValue = false,
    Callback = function(v) state.fly = v flyToggle(v) end
})
TabMove:CreateSlider({
    Name = "Fly Speed",
    Range = {10,200},
    Increment = 5,
    CurrentValue = 50,
    Callback = function(v) state.flySpeed = v end
})
TabMove:CreateSlider({
    Name = "WalkSpeed Boost",
    Range = {16,200},
    Increment = 2,
    CurrentValue = 16,
    Callback = function(v) 
        state.wsBoost = v 
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = v
        end
    end
})
TabMove:CreateToggle({
    Name = "Noclip",
    CurrentValue = false,
    Callback = function(v) state.noclip = v end
})
TabMove:CreateSlider({
    Name = "Gravity",
    Range = {0,400},
    Increment = 10,
    CurrentValue = Workspace.Gravity,
    Callback = function(v) Workspace.Gravity = v end
})
TabMove:CreateButton({
    Name = "Reset Gravity",
    Callback = function() Workspace.Gravity = 196.2 end
})

-- Aba Visual
local TabVisual = Window:CreateTab("Visual")
TabVisual:CreateToggle({
    Name = "ESP Players",
    CurrentValue = false,
    Callback = function(v) state.esp = v toggleESP(v) end
})
TabVisual:CreateToggle({
    Name = "FullBright",
    CurrentValue = false,
    Callback = function(v) state.fullbright = v toggleFullBright(v) end
})

-- Aba Proteções
local TabProtect = Window:CreateTab("Protections")
TabProtect:CreateToggle({
    Name = "Anti Sit v1",
    CurrentValue = false,
    Callback = function(v) state.antisit1 = v end
})
TabProtect:CreateToggle({
    Name = "Anti Sit v2",
    CurrentValue = false,
    Callback = function(v) state.antisit2 = v end
})
TabProtect:CreateButton({
    Name = "Anti Hack (limpa scripts)",
    Callback = function() antiHack() end
})
TabProtect:CreateToggle({
    Name = "Anti Collision",
    CurrentValue = false,
    Callback = function(v) state.anticollision = v end
})
TabProtect:CreateToggle({
    Name = "Anti Void",
    CurrentValue = false,
    Callback = function(v) state.antivoid = v toggleAntiVoid(v) end
})
TabProtect:CreateToggle({
    Name = "Anti Audio All",
    CurrentValue = false,
    Callback = function(v) state.antiaudio = v toggleAntiAudio(v) end
})
TabProtect:CreateToggle({
    Name = "Anti Lag",
    CurrentValue = false,
    Callback = function(v) state.antilag = v end
})

-- Aba Extra
local TabExtra = Window:CreateTab("Extras")
TabExtra:CreateButton({
    Name = "Respawn",
    Callback = function()
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.Health = 0
        end
    end
})
TabExtra:CreateButton({
    Name = "Teleport to Random Player",
    Callback = function()
        local others = {}
        for _,p in pairs(Players:GetPlayers()) do
            if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                table.insert(others, p)
            end
        end
        if #others > 0 then
            local target = others[math.random(1,#others)]
            LocalPlayer.Character:PivotTo(target.Character.HumanoidRootPart.CFrame + Vector3.new(2,0,2))
        end
    end
})
TabExtra:CreateButton({
    Name = "Godmode (simples)",
    Callback = function()
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.MaxHealth = math.huge
            LocalPlayer.Character.Humanoid.Health = math.huge
        end
    end
})
